function SaveMFASettings() {
	var email = $("#email_attribute").val();
	if (email.includes("@")) {
		$.ajax({
			method: "POST",
			url: "{{ sAjaxURL|raw }}",
			data: {
				"exec_module": {{ sModuleId }},
				"exec_page": "index.php",
				"operation": "UpdateMailSettings",
				"email": email
			}
		});
	}
}
